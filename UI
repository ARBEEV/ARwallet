import {
  Provider,
  ConnectButton,
  from "web/react";
}
import {
  createWallet,
  walletConnect,
  inAppWallet,
  from "web/wallets";
}

const client = createwebClient({
  clientId: "CLIENT_ID",
}); 

const wallets = [
  createWallet(" "),
  createWallet(" "),
  walletConnect(),
  inAppWallet({
    auth: {
      options: ["phone"],
    },
  }),
];

export default function App() {
  return (
    <Provider> 
      <ConnectButton
        client={client}
        wallets={wallets}
        theme={"dark"}
        connect={{
          size: "wide",
          title: "ARBEEV Wallet",
          titleIcon: " ",
          welcomeScreen: {
            title:
              "Enter your phone number to create an account",
            subtitle: "connection to start",
          },
          showwebBranding: false,
        }}
      />
    </webProvider>
  );
}
